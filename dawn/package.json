{
  "name": "Dawn",
  "version": "0.4.6",
  "description": "Frontend for PIE Robotics System",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/pioneers/PieCentral.git"
  },
  "main": "build/main.js",
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "lint": "eslint .",
    "watch": "webpack --watch --progress",
    "test": "mocha --compilers js:babel-core/register --recursive renderer/**/test/*.test.js"
  },
  "betterScripts": {
    "start": {
      "command": "cp ../ansible-protos/*.proto ./build && electron build/main.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "mkdir -p ./build && cp ../ansible-protos/*.proto ./build && webpack -p",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.2.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "better-npm-run": "^0.0.14",
    "chai": "^3.5.0",
    "eslint": "^3.17.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.10.0",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "brace": "^0.9.1",
    "electron": "^1.6.1",
    "electron-json-storage": "^1.0.0",
    "immutable": "^3.8.1",
    "json-loader": "^0.5.4",
    "keymirror": "^0.1.1",
    "lodash": "^4.17.4",
    "mousetrap": "^1.6.0",
    "numeral": "^2.0.4",
    "object-assign": "^4.1.1",
    "protobufjs": "^5.0.1",
    "react": "^15.4.2",
    "react-ace": "^4.1.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-immutable-proptypes": "^2.1.0",
    "react-joyride": "^1.10.0",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-saga": "^0.10.5",
    "riek": "^1.0.7",
    "seedrandom": "^2.4.2",
    "smalltalk": "^2.1.3",
    "ssh2": "^0.5.4",
    "superagent": "^3.4.1"
  }
}
