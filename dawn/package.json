{
  "name": "Dawn",
  "version": "2.0.0",
  "description": "Frontend for PIE Robotics System",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/pioneers/PieCentral.git"
  },
  "main": "build/main.js",
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "pkg": "node release.js",
    "lint": "eslint .",
    "watch": "webpack --mode development --watch --progress",
    "test": "mocha --require babel-core/register --recursive renderer/**/test/*.test.js",
    "release": "node release.js",
    "version": "echo $npm_package_version"
  },
  "betterScripts": {
    "start": {
      "command": "cp ../ansible-protos/*.proto ./build && electron build/main.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "mkdir -p ./build && cp ../ansible-protos/*.proto ./build && webpack --mode production --progress",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-json-strings": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "better-npm-run": "0.1.1",
    "chai": "4.2.0",
    "electron-devtools-installer": "2.2.4",
    "electron-packager": "^10.1.1",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "fs-extra": "5.0.0",
    "jszip": "3.1.5",
    "minimist": "1.2.0",
    "mocha": "7.0.1",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "zip-webpack-plugin": "3.0.0"
  },
  "dependencies": {
    "brace": "0.11.1",
    "bufferutil": "4.0.1",
    "create-react-class": "15.6.3",
    "electron": ">=8.0.0",
    "electron-json-storage": "4.1.8",
    "immutable": "3.8.2",
    "json-loader": "0.5.7",
    "keymirror": "0.1.1",
    "lodash": ">=4.17.13",
    "mousetrap": "1.6.1",
    "numeral": "2.0.6",
    "object-assign": "4.1.1",
    "prop-types": "15.7.2",
    "protobufjs": ">=6.8.8",
    "react": "16.12.0",
    "react-ace": "8.0.0",
    "react-bootstrap": "0.33.1",
    "react-dom": ">=16.12.0",
    "react-immutable-proptypes": "2.1.0",
    "react-joyride": "2.2.1",
    "react-redux": "7.1.3",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "runtime-client": "../runtime/runtime-node-client",
    "seedrandom": "3.0.5",
    "smalltalk": "4.0.2",
    "socket.io-client": "^2.3.0",
    "ssh2": "0.8.7",
    "superagent": "5.2.1",
    "utf-8-validate": "5.0.2",
    "ws": "7.2.1"
  }
}
