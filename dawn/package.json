{
  "name": "Dawn",
  "version": "2018.1.0",
  "description": "Frontend for PIE Robotics System",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/pioneers/PieCentral.git"
  },
  "main": "build/main.js",
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "lint": "eslint .",
    "watch": "webpack --mode development --watch --progress",
    "test": "mocha --require babel-core/register --recursive renderer/**/test/*.test.js",
    "release": "node release.js",
    "version": "echo $npm_package_version"
  },
  "betterScripts": {
    "start": {
      "command": "cp ../ansible-protos/*.proto ./build && electron build/main.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "mkdir -p ./build && cp ../ansible-protos/*.proto ./build && webpack --mode production --progress",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.3",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "better-npm-run": "0.1.0",
    "chai": "4.1.2",
    "electron-devtools-installer": "2.2.3",
    "electron-packager": "^10.1.1",
    "eslint": "4.18.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "fs-extra": "5.0.0",
    "jszip": "3.1.5",
    "minimist": "1.2.0",
    "mocha": "5.0.0",
    "webpack": "4.1.0",
    "webpack-cli": "2.0.10",
    "zip-webpack-plugin": "3.0.0"
  },
  "dependencies": {
    "brace": "0.11.0",
    "bufferutil": "3.0.3",
    "create-react-class": "15.6.2",
    "electron": "1.7.11",
    "electron-json-storage": "4.0.2",
    "immutable": "3.8.2",
    "json-loader": "0.5.7",
    "keymirror": "0.1.1",
    "lodash": "4.17.4",
    "mousetrap": "1.6.1",
    "numeral": "2.0.6",
    "object-assign": "4.1.1",
    "prop-types": "15.6.0",
    "protobufjs": "6.8.4",
    "react": "16.2.0",
    "react-ace": "5.9.0",
    "react-bootstrap": "0.32.0",
    "react-dom": "16.2.0",
    "react-immutable-proptypes": "2.1.0",
    "react-joyride": "1.11.1",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-saga": "0.16.0",
    "seedrandom": "2.4.3",
    "smalltalk": "2.5.1",
    "socket.io-client": "^2.0.4",
    "ssh2": "0.5.5",
    "superagent": "3.8.2",
    "utf-8-validate": "4.0.0",
    "ws": "4.0.0"
  }
}
